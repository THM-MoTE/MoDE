<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<!--suppress ALL -->
<fx:root prefWidth="700.0" styleClass="vbox-base" stylesheets="@../css/DialogStack.css" type="VBox"
         xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <fx:include fx:id="selectLibs" source="SelectLibs.fxml"/>
    <HBox alignment="CENTER" spacing="2.0" VBox.vgrow="ALWAYS">
      <children>
        <Button alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#onBtnBack" HBox.hgrow="NEVER">
          <graphic>
            <FontIcon iconColor="red" iconLiteral="gmi-arrow-back" iconSize="24"/>
          </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="btnNext" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#onBtnNext"
                HBox.hgrow="NEVER">
          <graphic>
            <FontIcon fx:id="fiNext" iconColor="green" iconLiteral="gmi-arrow-forward" iconSize="24"/>
          </graphic>
        </Button>
      </children>
      <padding>
        <Insets bottom="4.0" left="2.0" right="2.0" top="12.0"/>
      </padding>
    </HBox>
  </children>
  <padding>
    <Insets top="10.0"/>
  </padding>
</fx:root>
