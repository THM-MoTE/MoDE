<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<!--suppress ALL -->

<fx:root prefWidth="700.0" styleClass="vbox-base" stylesheets="@../../../control/css/DialogStack.css" type="VBox"
         xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <HBox>
      <children>
        <ListView fx:id="lvOptionList" prefHeight="350.0" prefWidth="220.0" HBox.hgrow="NEVER"/>
        <ScrollPane prefHeight="350.0" HBox.hgrow="ALWAYS">
          <AnchorPane fx:id="apRoot"/>
          <padding>
            <Insets left="-1.0" right="-1.0"/>
          </padding>
        </ScrollPane>
      </children>
    </HBox>
    <fx:include fx:id="dialogStackButtons" source="../../fragments/DialogStackButtons.fxml"/>
  </children>
</fx:root>
