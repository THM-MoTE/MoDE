<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<!--suppress ALL -->


<HBox alignment="CENTER" spacing="2.0" VBox.vgrow="ALWAYS"
      fx:controller="de.thm.mni.mote.mode.uiactor.controller.fragments.DialogStackButtonsController"
      xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Button fx:id="btnPrev" alignment="BOTTOM_LEFT" mnemonicParsing="false" HBox.hgrow="NEVER">
      <graphic>
        <FontIcon iconColor="red" iconLiteral="gmi-arrow-back" iconSize="24"/>
      </graphic>
    </Button>
    <Pane HBox.hgrow="ALWAYS"/>
    <Button fx:id="btnNext" alignment="BOTTOM_LEFT" mnemonicParsing="false" HBox.hgrow="NEVER">
      <graphic>
        <FontIcon fx:id="fiNext" iconColor="green" iconLiteral="gmi-arrow-forward" iconSize="24"/>
      </graphic>
    </Button>
  </children>
  <padding>
    <Insets bottom="4.0" left="3.0" right="3.0" top="12.0"/>
  </padding>
</HBox>