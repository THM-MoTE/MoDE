<?xml version="1.0" encoding="UTF-8"?>

<?import de.thm.mni.mhpp11.ui.control.TreeViewWithItems?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.*?>
<GridPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="700.0" prefWidth="1000.0" stylesheets="@../css/Main.css"
          xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="de.thm.mni.mhpp11.ui.controller.MainController">
  <columnConstraints>
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="5.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowSpan="2">
         <top>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <MenuBar>
                    <menus>
                      <Menu mnemonicParsing="false" text="%menu.file">
                        <items>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.file.new_project"
                                    accelerator="Shortcut+N">
                            <graphic>
                              <FontIcon iconColor="darkorange" iconLiteral="mdi-creation" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.file.open"
                                    accelerator="Shortcut+O">
                            <graphic>
                              <FontIcon iconColor="chocolate" iconLiteral="gmi-folder-open" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <Menu disable="true" mnemonicParsing="false" text="%menu.file.open_recent"/>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.file.close_project"/>
                          <SeparatorMenuItem mnemonicParsing="false"/>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.file.add_library">
                            <graphic>
                              <FontIcon iconColor="cadetblue" iconLiteral="gmi-add-to-photos" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <SeparatorMenuItem mnemonicParsing="false"/>
                          <MenuItem mnemonicParsing="false" onAction="#handleSave" text="%menu.file.save"
                                    accelerator="Shortcut+S">
                            <graphic>
                              <FontIcon iconColor="deepskyblue" iconLiteral="mdi-content-save" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <MenuItem mnemonicParsing="false" onAction="#handleSaveAll" text="%menu.file.save_all"
                                    accelerator="Shortcut+Shift+S">
                            <graphic>
                              <FontIcon iconColor="deepskyblue" iconLiteral="mdi-content-save-all" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <SeparatorMenuItem mnemonicParsing="false"/>
                          <MenuItem mnemonicParsing="false" text="%menu.file.exit" accelerator="Shortcut+X"/>
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="%menu.edit">
                        <items>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.edit.undo"
                                    accelerator="Shortcut+Z">
                            <graphic>
                              <FontIcon iconColor="mediumpurple" iconLiteral="gmi-undo" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <MenuItem disable="true" mnemonicParsing="false" text="%menu.edit.redo"
                                    accelerator="Shortcut+Y">
                            <graphic>
                              <FontIcon iconColor="mediumpurple" iconLiteral="gmi-redo" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <SeparatorMenuItem mnemonicParsing="false"/>
                          <MenuItem mnemonicParsing="false" text="%menu.edit.settings" accelerator="Shortcut+Alt+S">
                            <graphic>
                              <FontIcon iconColor="darkgrey" iconLiteral="gmi-settings" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="%menu.help">
                        <items>
                          <MenuItem mnemonicParsing="false" text="%menu.help.help" accelerator="F1">
                            <graphic>
                              <FontIcon iconColor="deepskyblue" iconLiteral="gmi-help" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                          <MenuItem mnemonicParsing="false" text="%menu.help.about">
                            <graphic>
                              <FontIcon iconColor="deepskyblue" iconLiteral="gmi-info" iconSize="18"/>
                            </graphic>
                          </MenuItem>
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
                 <!--<ToolBar prefHeight="40.0" prefWidth="200.0">
                    <items>
                       <BreadCrumbBar />
                    </items>
                 </ToolBar>-->
               </children>
            </VBox>
         </top>
         <bottom>
            <VBox minHeight="25.0" BorderPane.alignment="BOTTOM_LEFT">
               <children>
                  <Separator prefWidth="200.0" />
               </children>
            </VBox>
         </bottom>
         <center>
            <StackPane>
               <children>
                  <GridPane>
                     <columnConstraints>
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                     </rowConstraints>
                     <children>
                        <VBox GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <children>
                              <Label text="Welcome">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </children>
                  </GridPane>
                 <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0">
  
                 </TabPane>
               </children>
            </StackPane>
         </center>
         <left>
            <HBox fx:id="hbLeft" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
                     <children>
                       <!--                        <ToolBar visible="false">
                                                  <items>
                                                     <TextField fx:id="tfLibFilter" alignment="TOP_LEFT" promptText="Filter" visible="false" />
                                                  </items>
                                               </ToolBar>-->
                        <HBox VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
                           </VBox.margin>
                           <children>
                              <ScrollPane fitToHeight="true" fitToWidth="true" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                                 <content>
                                   <TreeViewWithItems fx:id="tvLibrary" prefHeight="200.0" prefWidth="200.0">
                                       <padding>
                                          <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
                                       </padding>
                                   </TreeViewWithItems>
                                 </content>
                              </ScrollPane>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Separator fx:id="sLeft" halignment="LEFT" nodeOrientation="RIGHT_TO_LEFT" orientation="VERTICAL">
                     <padding>
                        <Insets right="1.0" />
                     </padding></Separator>
               </children>
               <BorderPane.margin>
                  <Insets right="-1.0" />
               </BorderPane.margin>
            </HBox>
         </left>
         <right>
           <HBox fx:id="hbRight" maxWidth="0.0" minWidth="0.0" prefHeight="606.0" prefWidth="1.0" visible="false"
                 BorderPane.alignment="CENTER">
               <children>
                  <Separator fx:id="sRight" nodeOrientation="LEFT_TO_RIGHT" orientation="VERTICAL" />
                  <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
                     <children>
                        <ToolBar>
                           <items>
                              <Button mnemonicParsing="false" text="Button" />
                           </items>
                        </ToolBar>
                        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
                           </VBox.margin>
                        </ScrollPane>
                     </children>
                  </VBox>
               </children>
               <BorderPane.margin>
                  <Insets left="-1.0" />
               </BorderPane.margin>
            </HBox>
         </right>
      </BorderPane>
      <VBox fx:id="notifications" alignment="BOTTOM_RIGHT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</GridPane>
